{
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.fusiongenerator.fun"
                }
            ],
            "destination": "https://fusiongenerator.fun/:path*",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "all"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}